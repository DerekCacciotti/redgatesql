+			   substring((select ','+LTRIM(RTRIM(STR(HVProgramPK)))
+							  from HVProgram
+							  for xml path ('')),2,8000)
+	end
+	set @programfk = REPLACE(@programfk,'"','');
+
+	set @SiteFK = case when dbo.IsNullOrEmpty(@SiteFK) = 1 then 0
