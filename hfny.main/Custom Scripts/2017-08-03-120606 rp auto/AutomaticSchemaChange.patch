+
+
+		insert	into #tblInitial_cohort
+				select	[HVCasePK]
+					  , [CaseProgress]
+					  , [Confidentiality]
+					  , [CPFK]
+					  , [DateOBPAdded]
+					  , [EDC]
+					  , [FFFK]
