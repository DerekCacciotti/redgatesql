+CREATE TRIGGER [dbo].[fr_Kempe_Edit]
+on [dbo].[Kempe]
+AFTER UPDATE
+
+AS
+
+Declare @PK int
+Declare @UpdatedFormDate datetime 
+Declare @FormTypeValue varchar(2)
+
